{
  "parser": "babel-eslint",
  "root": true,
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "impliedStrict": true
    },
  },
  "rules": {
    "no-invalid-this": 2,

    "strict": [0, "global"],
    "global-strict": [0, "always"],
  }
}
